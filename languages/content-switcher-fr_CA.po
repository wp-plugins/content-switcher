msgstr "Administrateurs"

msgid "Editors"
msgstr "Éditeurs"

msgid "Authors"
msgstr "Auteurs"

msgid "Contributors"
msgstr "Contributeurs"

msgid "Subscribers"
msgstr "Abonnés"

msgid "Visitors without any role"
msgstr "Visiteurs sans aucun rôle"

msgid "you can check several boxes"
msgstr "vous pouvez cocher plusieurs cases"

msgid "Add JavaScript code"
msgstr "Ajouter le code JavaScript"

msgid "If you uncheck this box, Content Switcher will never add any JavaScript code to the pages of your website, but Google Analytics and Google Optimizer will not work."
msgstr "Si vous décochez cette case, Content Switcher n'ajoutera jamais de code JavaScript aux pages de votre site Web, mais Google Analytics et Google Optimizer ne fonctionneront pas."

msgid "<a href=\"http://www.kleor-editions.com/content-switcher/en/#part6.1\">More informations</a>"
msgstr "<a href=\"http://www.kleor-editions.com/content-switcher/#part6.1\">Plus d'informations</a>"

